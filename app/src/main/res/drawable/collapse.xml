<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="25dp"
                android:height="25dp"></size>
 			<solid android:color="#F8F8F8"></solid>
            <stroke android:color="#41C7FE" android:width="1dp"></stroke>
        </shape>
    </item>

    <item  android:gravity="center" android:width="15dp">
        <shape android:shape="line">
            <stroke
                android:width="1dp"
                android:color="#0178B9" />

        </shape>
    </item>
</layer-list>